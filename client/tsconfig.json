{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "declaration": true,
    "sourceMap": true,
    "outDir": "out",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "lib": [ "es2016" ],
    "newLine": "LF"
  },
  "exclude": [
    "node_modules",
    "server"
  ]
}
